#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
    {
    int t,i;
    unsigned int n,k;
    scanf("%d",&t);
    for(i=0;i<t;++i)
        {
        scanf("%d",&n);
            k = ~n ;
        printf("%u\n",k);
        }

        

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
    }
